<h2>Show Attendance</h2>
<%= @course.code %> - <%= @course.title %>

<table>
    <tr>
        <th></th>
        <% @date_list.each do |d| %>
            <th><%= d %></th>
        <% end %>
    </tr>
    <% @attendance_list.each do |att| %>
        <tr>
            <td><%= att[:name] %></td>
            <% att[:attendance].each do |a| %>
                <td><%= a.attendance %></td>
            <% end %>
        </tr>
    <% end %>
</table>

<br><br><%= link_to "<< Back", course_path %>